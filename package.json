{
  "name": "cloudcom-teamup",
  "version": "1.0.0",
  "description": "Cloudcom TeamUp",
  "private": true,
  "workspaces": [
    "teamup_back",
    "teamup_front"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run start -w teamup_back",
    "dev:frontend": "npm run dev -w teamup_front",
    "build:frontend": "npm --workspace=teamup_front run build",
    "build:backend": "npm --workspace=teamup_back run build || echo 'no backend build script'",
    "build": "npm run build:frontend"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  },
  "keywords": [
    "teamup",
    "cognito",
    "nextjs",
    "express"
  ],
  "author": "Cloudcom Team",
  "license": "MIT"
}
